{%- comment -%} "theme-default-pagination" this Class is needed for active page pagination button disable{%- endcomment -%}
{% if paginate.pages > 1 %}
<div class="row">
  <div class="col-12">
    <div class="theme-default-pagination">
        <ul class="pagination mt-3 justify-content-center pagination_style1">
          {% if paginate.previous %}
          <li class="page-item">
            <a href="{{ paginate.previous.url }}" aria-label="Previous">
              <i class="linearicons-arrow-left"></i>
            </a>
          </li>
          {% else %}
          <li class="disabled prev page-item">
            <a href="#">
              <i class="linearicons-arrow-left"></i>
            </a>
          </li>
          {% endif %}
          {% for part in paginate.parts %}
          {% if part.is_link %}
          <li class="page-item">
            <a class="page-link" href="{{ part.url }}" title="">{{ part.title }}</a>
          </li>
          {% else %}
          {% if part.title == paginate.current_page %}
          <li class="page-item active"><a class="page-link" href="">{{ part.title }}</a></li>
          {% else %}
          <li><a class="page-link" href="#">{{ part.title }}</a></li>
          {% endif %}
          {% endif %}
          {% endfor %}
          {% if paginate.next %}
          <li class="next page-item"><a href="{{ paginate.next.url }}" title="{{ paginate.next.title }}"><i class="linearicons-arrow-right"></i></a></li>
          {% else %}
          <li class="disabled next page-item">
            <a href="#">
              <i class="linearicons-arrow-right"></i>
            </a>
          </li>
          {% endif %}
        </ul>
    </div>
  </div>
</div>
{% endif %}






<script>
  $(".theme-default-pagination .disabled a").removeAttr("href");
  $(".theme-default-pagination li.active a").removeAttr("href");
</script>